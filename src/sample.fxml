<VBox maxHeight="485.0" maxWidth="601.0" minHeight="400.0" minWidth="600" prefHeight="485.0" prefWidth="601.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller">
